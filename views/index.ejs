    <script>

        // Function to validate form inputs




        $(document).ready(function () {


            // Dat eof birth varification
            document.getElementById('dob').addEventListener('change', validateAge);
            document.getElementById('gender').addEventListener('change', validateAge);

            function validateAge() {
                const gender = document.getElementById('gender').value;
                const dob = document.getElementById('dob').value;
                const genderError = document.getElementById('genderError');
                const dobError = document.getElementById('dobError');

                // Clear previous error messages
                genderError.textContent = "";
                dobError.textContent = "";

                // Exit if no gender or date of birth is selected
                if (!gender || !dob) {
                    return;
                }

                // Calculate age
                const birthDate = new Date(dob);
                const today = new Date();
                let age = today.getFullYear() - birthDate.getFullYear();
                const monthDiff = today.getMonth() - birthDate.getMonth();
                const dayDiff = today.getDate() - birthDate.getDate();

                if (monthDiff < 0 || (monthDiff === 0 && dayDiff < 0)) {
                    age--;
                }

                let minAge = 0;
                if (gender === "Male") {
                    minAge = 21;
                } else if (gender === "Female") {
                    minAge = 18;
                } else {
                    // No validation required for "Others"
                    return;
                }

                // Display error if age is less than the required minimum
                if (age < minAge) {
                    dobError.textContent = `Minimum age for ${gender} is ${minAge}. Please select a valid date of birth.`;
                    dobError.classList.add('text-red-500');
                }
            }


            // Mapping of employment types to occupations
            const occupationMapping = {
                government: ["Civil Servant", "Government Officer", "PSU Employee", "Bank Officer", "RBI Officer", "Other"],
                private: ["Corporate Employee", "Manager", "Software Engineer", "Investment Banker", "Financial Analyst", "Banker", "Risk Manager", "Loan Officer", "Other"],
                business: ["Business Owner", "Entrepreneur", "Franchise Owner", "Other"],
                defence: ["Army Officer", "Navy Officer", "Air Force Officer", "Other"],
                selfEmployed: ["Freelancer", "Consultant", "Contractor", "Other"],
                notWorking: ["Student", "Homemaker", "Retired", "Other"]
            };

            // Event listener for when the "Employed In" dropdown changes
            document.getElementById("employedIn").addEventListener("change", function () {
                const selectedEmployedIn = this.value;
                const occupationDropdown = document.getElementById("occupation");
                const exceptionInput = document.getElementById("exceptionInput");

                // Clear previous occupation options
                occupationDropdown.innerHTML = '<option value="">Occupation</option>';

                // Disable the occupation dropdown if no "Employed In" is selected
                if (!selectedEmployedIn) {
                    occupationDropdown.disabled = true;
                    exceptionInput.style.display = "none";
                    // Text the error message
                    document.getElementById("occupationError").textContent =
                        "Please select an employment type first.";
                    document.getElementById("occupationError").classList.add("text-red-500");
                    return;
                }

                // Enable the occupation dropdown if an "Employed In" is selected
                occupationDropdown.disabled = false;

                // Hide the exception input by default
                // exceptionInput.style.display = "none";

                // If the selected "Employed In" category has a mapping, update the "Occupation" dropdown
                if (occupationMapping[selectedEmployedIn]) {
                    occupationMapping[selectedEmployedIn].forEach(function (occupation) {
                        const option = document.createElement("option");
                        option.value = occupation;
                        option.textContent = occupation;
                        occupationDropdown.appendChild(option);
                    });
                }
            });

            // Event listener for when the "Occupation" dropdown changes
            // document.getElementById("occupation").addEventListener("change", function () {
            //     const selectedOccupation = this.value;
            //     const exceptionInput = document.getElementById("exceptionInput");

            //     // Show the exception input field if "Other" is selected
            //     if (selectedOccupation === "Other") {
            //         exceptionInput.style.display = "block";
            //     } else {
            //         exceptionInput.style.display = "none";

            //     }
            // });
            // Event listener for when the "exception" input is typed into
            // document.getElementById("exception").addEventListener("input", function () {
            //     const inputField = this;

            //     // Check if the field is empty
            //     if (inputField.value.trim() === "") {
            //         inputField.classList.remove("success");
            //         inputField.classList.remove("");
            //         inputField.classList.add("error");
            //         inputField.getAttribute('required');
            //     } else {
            //         inputField.classList.remove("error");
            //         inputField.classList.remove("exception-input");
            //         inputField.classList.add("success");
            //         // Remove the equire attribute
            //         inputField.removeAttribute('required');
            //     }
            // });

            // Form 
            $('#clickToDisplayForm').click(function () {
                // Show the form
                $('#form-div').removeClass('d-none');
                $('#clickToDisplayForm').addClass('hide');
                $('#left-content').addClass('hide');

            });

            // function clickToDisplay() {
            //     console.log('Click to display');
            //     $('#form-container').removeClass('d-none');
            // }
            // Add event listener to maintain "filled" state
            const selects = document.querySelectorAll('.form-select');
            selects.forEach((select) => {
                select.addEventListener('change', () => {
                    if (select.value) {
                        select.setAttribute('data-filled', 'true');
                    } else {
                        select.removeAttribute('data-filled');
                        // Dynamic id attribute error showing
                        // text field in the error div

                    }
                });
            });
            // Multi- level Form elements
            const sections = $(".form-section");
            let currentSection = 0;

            function showSection(index) {                   // 0 is in the index
                sections.removeClass("active");
                sections.eq(index).addClass("active");
                $("#prevButton").prop("disabled", index === 0);
                $("#nextButton").toggle(index < sections.length - 1);
                $("#submitButton").toggle(index === sections.length - 1);
            }

            function validateSection(index) {
                let isValid = true;
                sections.eq(index).find(".error").text("");

                sections.eq(index).find("input[required], select[required]").each(function () {
                    const input = $(this);
                    // console.log('cu', input.val());
                    const value = input.val();
                    const id = input.attr("id");

                    console.log("value is : ", value);

                    // Basic validation for required fields
                    if (input.attr("required")) {
                        if (input.attr("required") && value === "" || value === "null") {
                            $(`#${id}Error`).text("This field is required.");
                            isValid = false;
                        }
                    }
                    if (value === "" || value === "null") {
                        $(`#${id}Error`).text("This field is required.");
                        isValid = false;
                    }
                    if (id === "gender" && (value === "null" || value === "")) {
                        $(`#${id}Error`).text("Please select a valid gender.");
                        isValid = false;
                    }
                    if (id === "phone" && value !== "" && !/^[0-9]{10}$/.test(value)) {
                        $(`#${id}Error`).text("Phone number must be 10 digits.");
                        isValid = false;
                    }

                    if (id === "email" && value !== "" && !/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(value)) {
                        $(`#${id}Error`).text("Please enter a valid email address.");
                        isValid = false;
                    }
                    // if (password != confirmPassword) {
                    //     $("#passwordError").text("Passwords do not match.");
                    //     isValid = false;
                    // }
                    if (id === "annualIncomexx" && value === "null") {
                        console.log('from annuIncome')
                        $(`#${id}Error`).text("Please select your annual income.");
                        isValid = false;
                    }

                });

                return isValid;
            }


            // 



            $("#nextButton").on("click", function () {
                if (validateSection(currentSection)) {
                    currentSection++;
                    showSection(currentSection);
                }
            });

            $("#prevButton").on("click", function () {
                currentSection--;
                showSection(currentSection);
            });

            showSection(currentSection);


            const sendOtpButton = document.getElementById("sendOtpButton");
            const resendOtpButton = document.createElement("button");
            const otpInput = document.getElementById("otpInput");
            const verifyOtpButton = document.getElementById("verifyOtpButton");
            const otpError = document.getElementById("otpError");
            let timer;

            // Initial states
            resendOtpButton.id = "resendOtpButton";
            resendOtpButton.className = "btn btn-secondary mt-2 w-full";
            resendOtpButton.style.display = "none";
            resendOtpButton.innerText = "Resend OTP";
            sendOtpButton.parentElement.appendChild(resendOtpButton);

            // Send OTP handler
            sendOtpButton.addEventListener("click", async () => {
                const phone = document.getElementById("phone").value;
                if (!phone) {
                    otpError.innerText = "Phone number is required.";
                    return;
                }

                try {
                    const response = await fetch("/send-otp", {
                        method: "POST",
                        headers: { "Content-Type": "application/json" },
                        body: JSON.stringify({ phone }),
                    });

                    const result = await response.json();
                    if (response.ok) {
                        otpError.innerText = "OTP sent successfully!";
                        startTimer(30);
                        otpInput.disabled = false;
                        verifyOtpButton.disabled = false;
                        sendOtpButton.disabled = true;
                        resendOtpButton.style.display = "none";
                    } else {
                        otpError.innerText = result.error || "Failed to send OTP.";
                    }
                } catch (error) {
                    otpError.innerText = "An error occurred. Please try again.";
                }
            });

            // Resend OTP handler
            resendOtpButton.addEventListener("click", async () => {
                sendOtpButton.click();
            });

            // Timer function
            function startTimer(seconds) {
                let remaining = seconds;
                otpError.innerText = `You can resend OTP in ${remaining} seconds.`;
                resendOtpButton.style.display = "none";

                timer = setInterval(() => {
                    remaining--;
                    if (remaining <= 0) {
                        clearInterval(timer);
                        otpError.innerText = "";
                        resendOtpButton.style.display = "block";
                    } else {
                        otpError.innerText = `You can resend OTP in ${remaining} seconds.`;
                    }
                }, 1000);
            }

            // Verify OTP handler
            verifyOtpButton.addEventListener("click", async () => {
                const phone = document.getElementById("phone").value;
                const otp = otpInput.value;

                if (!otp) {
                    otpError.innerText = "Please enter the OTP.";
                    return;
                }

                try {
                    const response = await fetch("/verify-otp", {
                        method: "POST",
                        headers: { "Content-Type": "application/json" },
                        body: JSON.stringify({ phone, otp }),
                    });

                    const result = await response.json();
                    if (response.ok) {
                        otpError.innerText = "OTP verified successfully!";
                        otpInput.disabled = true;
                        verifyOtpButton.disabled = true;
                        resendOtpButton.style.display = "none";
                    } else {
                        otpError.innerText = result.error || "Failed to verify OTP.";
                    }
                } catch (error) {
                    otpError.innerText = "An error occurred. Please try again.";
                }
            });

            // Submit the Form Finally
            $("#multiStepForm").on("submit", function (e) {
                e.preventDefault();



                if ($("#submitButton").is(":disabled")) {
                    alert("Please verify OTP before submitting the form.");
                } else {
                    // Proceed with form submission
                    $.ajax({
                        url: '/submit-form', // Replace with your form submission endpoint
                        method: 'POST',
                        data: $(this).serialize(),
                        success: function (response) {
                            // alert("Form submitted successfully!");
                            // Hide the error alert if visible
                            $('#formError').addClass('d-none');

                            $('#formSuccess')
                                .text("You are successfully Registered. Redirecting for Login...")
                                .removeClass('d-none');
                            // Redirect to the Dashboard
                            setTimeout(function () { window.location.href = "/login"; }, 1000); // Redirect after 1 seconds
                        },
                        error: function (xhr) {
                            // Hide the success alert if visible
                            $('#formSuccess').addClass('d-none');

                            const error = xhr.responseJSON.error || 'An unexpexted error occurred'
                            $('#formError')
                                .text("Error: " + error)
                                .removeClass('d-none');
                        }
                    });
                }
            });

            // Modal For Login
            document.getElementById("loginForm").addEventListener("submit", function (e) {
                // e.preventDefault();
                const email = document.getElementById("email").value.trim();
                const password = document.getElementById("password").value.trim();

                if (!email || !password) {
                    document.getElementById("emailError").textContent = !email ? "Email is required" : "";
                    document.getElementById("passwordError").textContent = !password ? "Password is required" : "";
                    return;
                }
            });



            // Validation of annual income
            $("#annualIncome").on("change", function () {
                const annualIncome = $(this).val();
                if (annualIncome) {
                    // Clear error message if a valid option is selected
                    $("#annualIncomeError").text("");
                } else {
                    // Show error message if no value is selected
                    $("#annualIncomeError").text("Please select your annual income.");
                }
            });

        });
    </script>
</body>

</html>